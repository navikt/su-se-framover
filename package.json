{
    "name": "su-se-framover",
    "version": "1.0.0",
    "private": true,
    "description": "Frontend for saksbehandlingsstøtteverktøy for supplerende stønad",
    "browserslist": [
        "> 0.25%, last 2 versions, not dead"
    ],
    "type": "module",
    "scripts": {
        "dev": "vite",
        "eslint": "eslint src --report-unused-disable-directives",
        "eslint-fix": "eslint src --fix",
        "lint": "npm run eslint",
        "preview": "vite preview",
        "postinstall": "cd server && npm ci",
        "start": "NODE_ENV=development nodemon && vite",
        "test": "jest --passWithNoTests",
        "build": "vite build",
        "build:all": "npm run build:server && npm run build",
        "build:server": "npm run build --prefix ./server",
        "lint-and-typecheck": "npm run lint && npm run typecheck",
        "typecheck": "tsc --project tsconfig.json --noEmit && tsc --project server/tsconfig.json --noEmit",
        "tdd": "jest --watch",
        "prettier": "prettier --write src server scripts",
        "prepare": "npx husky install",
        "tsc:watch": "tsc --watch --preserveWatchOutput"
    },
    "lint-staged": {
      "*.{js,jsx,ts,tsx,md,html,css,less}": [
        "biome check"
      ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/su-se-framover.git"
    },
    "author": "supstonad",
    "license": "MIT",
    "engines": {
        "node": ">=24.1.0 <25.0.0",
        "npm": ">=11.3.0"
    },
    "bugs": {
        "url": "https://github.com/navikt/su-se-framover/issues"
    },
    "homepage": "https://github.com/navikt/su-se-framover#readme",
    "dependencies": {
        "@devexperts/remote-data-ts": "2.1.1",
        "@formatjs/intl-datetimeformat": "6.18.0",
        "@formatjs/intl-numberformat": "8.15.4",
        "@hookform/resolvers": "3.1.0",
        "@navikt/aksel-icons": "7.33.0",
        "@navikt/ds-css": "7.33.0",
        "@navikt/ds-react": "7.33.0",
        "@reduxjs/toolkit": "2.10.1",
        "@types/jest": "30.0.0",
        "@types/react-dom": "^19.2.2",
        "classnames": "2.5.1",
        "country-data-list": "1.4.1",
        "date-fns": "4.1.0",
        "fp-ts": "2.16.10",
        "io-ts": "2.2.22",
        "io-ts-types": "0.5.19",
        "lodash.debounce": "4.0.8",
        "lodash.isequal": "4.5.0",
        "react": "19.2.0",
        "react-dom": "19.2.0",
        "react-hook-form": "7.66.0",
        "react-intl": "7.1.14",
        "react-redux": "9.2.0",
        "react-router-dom": "7.9.5",
        "redux": "5.0.1",
        "reset-css": "5.0.2",
        "uuid": "11.1.0",
        "yup": "0.29.3"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.4",
        "@eslint/compat": "^1.4.1",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.39.1",
        "@types/lodash.debounce": "4.0.9",
        "@types/lodash.isequal": "4.5.8",
        "@types/react": "19.2.2",
        "@types/react-router-dom": "5.3.3",
        "@types/uuid": "10.0.0",
        "@types/yup": "0.29.13",
        "@typescript-eslint/eslint-plugin": "8.46.2",
        "@typescript-eslint/parser": "8.46.2",
        "@vitejs/plugin-react": "5.0.1",
        "eslint": "9.39.0",
        "eslint-import-resolver-typescript": "4.4.4",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-jest": "^29.0.1",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-prettier": "5.5.4",
        "eslint-plugin-react": "7.37.5",
        "globals": "^16.5.0",
        "husky": "9.1.7",
        "jest": "30.2.0",
        "less": "4.4.1",
        "lint-staged": "16.2.6",
        "nodemon": "3.1.10",
        "prettier": "3.6.2",
        "ts-jest": "29.4.5",
        "ts-node": "~10.9.2",
        "tsx": "4.19.4",
        "typescript": "5.8.3",
        "vite": "7.1.11",
        "vite-tsconfig-paths": "5.1.4"
    },
    "overrides": {
        "yaml@<2.2.2": "2.2.2"
    }
}
