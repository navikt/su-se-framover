{
  "name": "su-se-framover",
  "version": "1.0.0",
  "description": "Frontend for saksbehandlingsstøtteverktøy for supplerende stønad",
  "scripts": {
    "test": "jest",
    "tdd": "jest --watch",
    "start-no-browser": "npm run inject_local_config && parcel src/index.html",
    "start": "npm run start-no-browser -- --open",
    "build": "npm run clean:dist && parcel build src/index.html --out-dir dist/ --no-source-maps",
    "inject_local_config": "cp config.dev.json dist/config.json",
    "clean:dist": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/su-se-framover.git"
  },
  "author": "supstonad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/navikt/su-se-framover/issues"
  },
  "homepage": "https://github.com/navikt/su-se-framover#readme",
  "dependencies": {
    "@babel/runtime": "^7.8.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-transform-runtime": "^7.8.0",
    "@babel/preset-react": "^7.8.0",
    "@testing-library/react-hooks": "^3.2.1",
    "jest": "^24.9.0",
    "parcel-bundler": "^1.12.4",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0"
  }
}
