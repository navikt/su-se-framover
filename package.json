{
    "name": "su-se-framover",
    "version": "1.0.0",
    "description": "Frontend for saksbehandlingsstøtteverktøy for supplerende stønad",
    "browserslist": [
        "> 0.25%, last 2 versions, not dead"
    ],
    "source": "src/index.js",
    "scripts": {
        "postinstall": "cd server && npm install",
        "start": "rimraf ./.parcel-cache && rimraf ./server/.parcel-cache && nodemon",
        "test": "jest --passWithNoTests",
        "build": "npm run clean:dist && rimraf ./.parcel-cache && npm run build:client",
        "build:server": "npm run build --prefix ./server",
        "build:client": "SENTRY_RELEASE=$(git rev-parse --short HEAD) parcel build --no-scope-hoist --no-autoinstall src/index.html",
        "sentry-release": "SENTRY_RELEASE=$(git rev-parse --short HEAD) node scripts/sentry-release",
        "lint-and-typecheck": "npm run lint && npm run typecheck",
        "lint": "npm run eslint",
        "eslint": "eslint src --ext .tsx,.ts,.jsx,.js",
        "typecheck": "tsc && cd server && tsc",
        "tdd": "jest --watch",
        "clean:dist": "rimraf dist",
        "prettier": "prettier --write src server",
        "prepare": "npx husky install"
    },
    "lint-staged": {
        "*.{js,ts,tsx}": "eslint --cache --fix",
        "*.{js,jsx,ts,tsx,md,html,css,less}": "prettier --write"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/su-se-framover.git"
    },
    "author": "supstonad",
    "license": "MIT",
    "engines": {
        "node": ">=16",
        "npm": ">=8"
    },
    "bugs": {
        "url": "https://github.com/navikt/su-se-framover/issues"
    },
    "homepage": "https://github.com/navikt/su-se-framover#readme",
    "dependencies": {
        "@devexperts/remote-data-ts": "^2.1.1",
        "@formatjs/intl-datetimeformat": "^6.10.0",
        "@formatjs/intl-numberformat": "^8.7.0",
        "@hookform/resolvers": "^3.1.0",
        "@navikt/ds-css": "^4.6.1",
        "@navikt/ds-css-internal": "^3.4.3",
        "@navikt/ds-icons": "^3.4.3",
        "@navikt/ds-react": "^4.6.1",
        "@navikt/ds-react-internal": "^3.4.3",
        "@reduxjs/toolkit": "^1.9.5",
        "@sentry/browser": "^7.57.0",
        "@sentry/cli": "^2.19.4",
        "@types/jest": "^29.5.2",
        "amplitude-js": "^8.21.9",
        "autoprefixer": "^10.4.14",
        "classnames": "^2.3.2",
        "core-js": "^3.32.0",
        "country-data-list": "^1.3.0",
        "date-fns": "^2.30.0",
        "dayjs": "^1.11.9",
        "fp-ts": "^2.16.0",
        "io-ts": "^2.2.20",
        "io-ts-types": "^0.5.19",
        "lodash.debounce": "^4.0.8",
        "lodash.isequal": "^4.5.0",
        "lodash.throttle": "^4.1.1",
        "process": "^0.11.10",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-hook-form": "^7.45.1",
        "react-intl": "^6.4.4",
        "react-modal": "^3.16.1",
