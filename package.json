{
    "name": "su-se-framover",
    "version": "1.0.0",
    "description": "Frontend for saksbehandlingsstøtteverktøy for supplerende stønad",
    "scripts": {
        "lint": "tsc --noEmit && npm run eslint",
        "eslint": "eslint src --ext .tsx,.ts,.jsx,.js",
        "test": "jest --passWithNoTests",
        "tdd": "jest --watch",
        "start-no-browser": "npm run inject_local_config && parcel src/index.html",
        "start": "npm run start-no-browser -- --open",
        "build": "npm run clean:dist && npm run lint && npm run test && parcel build src/index.html --out-dir dist/ --no-source-maps",
        "inject_local_config": "cp config.dev.json dist/config.json || echo \"Kjør 'npm run build' en gang først for å fikse serveroppsettet.\"",
        "clean:dist": "rimraf dist",
        "prettier": "prettier --write src/**/*.{ts,tsx,scss,css,json,less}"
    },
    "husky": {
        "hooks": {
            "pre-push": "npm run lint",
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": "eslint --cache --fix",
        "*.{js,jsx,ts,tsx,md,html,css,less}": "prettier --write"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/su-se-framover.git"
    },
    "author": "supstonad",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/navikt/su-se-framover/issues"
    },
    "homepage": "https://github.com/navikt/su-se-framover#readme",
